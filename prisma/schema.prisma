generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id         String   @id @default(uuid())
  first_name String
  last_name  String
  age        Int
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt

  @@map("user")
}
